{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "npm run sync-config && npm run build-web",
    "devPath": "../",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "vtproxy",
    "version": "0.0.5"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": "^https://(www\\.)?virustotal\\.com/.*|^https://(www\\.)?orangecyberdefense\\.com/.*"
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.orangecyberdefense.vtproxy",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Â© 2025 Orange Cyberdefense CERT",
      "category": "DeveloperTool",
      "shortDescription": "VirusTotal proxy interface",
      "longDescription": "A lightweight desktop application for analyzing files, URLs, domains, and IPs using VirusTotal API.",
      "deb": {
        "depends": []
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "VTproxy - VirusTotal proxy interface",
        "width": 1280,
        "height": 900,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false,
        "theme": "Dark"
      }
    ]
  }
}
